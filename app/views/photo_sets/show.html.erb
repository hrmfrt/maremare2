<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @photo_sets.find(params[:id]).name%>
</p>

<p>
  <strong>Portrait:</strong>
  <% if @photo_sets.find(params[:id]).image_sets.attached? %>
    <% @photo_sets.find(params[:id]).image_sets.each do |photo| %>
      <%= image_tag photo.variant(resize_to_limit: [100, 100]) %>
    <% end %>
  <% end %>
</p>


<p>にゃん</p>


<%= link_to 'Edit', edit_photo_set_path(@photo_sets) %> |
<%= link_to 'Back', photo_sets_path %>
