<%= render './maregiment/maregiment_header'%>

<p id="notice"><%= notice %></p>

<h1>カテゴリー2</h1>

<%= link_to 'カテゴリー2を追加する', new_category_second_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_seconds.each do |category_second| %>
      <tr>
        <td><%= category_second.name %></td>
        <td><%= category_second.order %></td>
        <td><%= link_to 'Show', category_second %></td>
        <td><%= link_to 'Edit', edit_category_second_path(category_second) %></td>
        <td><%= link_to 'Destroy', category_second, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= form_tag import_category_seconds_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "インポート" %>
<% end %>
<br>



